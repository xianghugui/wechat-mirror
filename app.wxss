/**app.wxss**/
@import "static/fontAwesome.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  
} 

page{
  font-family: "Microsoft YaHei","SimSun","SimHei","Hiragino Sans GB",arial,sans-serif;
}

.page{
  position:fixed;
  width:100%;
  height:100%;
  color:#2e2e2e;
}
::-webkit-scrollbar{
  width:0;
  height:0;
  color:transparent;
}
.navigator-arrow {
  padding-right: 26rpx;
}
.navigator-arrow:after {
  content: " ";
  display: inline-block;
  height: 18rpx;
  width: 18rpx;
  border-width: 2rpx 2rpx 0 0;
  border-color: #888888;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -8rpx;
  right: 28rpx;
}