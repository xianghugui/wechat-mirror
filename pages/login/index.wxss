/* pages/login/index.wxss */
form{
  position: absolute;
  left: 0; top: 0; right: 0; bottom: 0;
  width: 750rpx;
  height: 800rpx;
  margin: auto;
}
image{
  width: 200rpx;
  height: 200rpx;
  border-radius: 100rpx;
  margin-left: 275rpx;
  margin-bottom: 80rpx;
}
.btn-area{
  width: 100%;
  height: 86rpx;
  display: flex;
  justify-content: center;
  margin-top: 60rpx;
}
button{
  width: 462rpx;
  height: 86rpx;
  line-height: 86rpx;
  border-radius: 86rpx;
  text-align:center;
  color: white;
  background-color: rgb(75, 194, 201);
  transform:scale(1.2);
  font-size: 32rpx;
}
.inputLint{
  padding: 0 100rpx;
  width: 750rpx;
  height: 106rpx;
  margin-top: 20rpx;
}
.inputTitle{
  width: 100rpx;
  margin-right: 20rpx;
  float: left;
  font-size: 35rpx;
}

.inputborder{
  width: 550rpx;
  height: 80rpx;
  border-bottom: 1rpx solid #f3f3f3;
}
input{
  width: 430rpx;
  font-size: 35rpx;
}
radio{
  transform:scale(0.7);
}
.protocol{
  width: 750rpx;
  height: 80rpx;
  display: flex;
  font-size: 28rpx;
  align-items: center;
  padding: 0 100rpx;
}
navigator{
  color: rgb(75, 194, 201);
}