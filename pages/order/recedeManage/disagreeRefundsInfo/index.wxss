  height: 30rpx;
  border-radius: 15rpx;
  background-color: red;
  color: white;
  font-size: 30rpx;
  text-align: center;
  line-height: 30rpx;
}

.disagree{
  width: 560rpx;
  margin-top: 100rpx;
  background-color: #49c4cb;
  border-radius: 40rpx;
