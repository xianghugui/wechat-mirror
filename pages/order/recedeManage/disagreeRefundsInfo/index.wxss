page{
  background-color: #f3f3f3;
}
.input-div{
  width: 100%;
  height: 200rpx;
  background-color: #ffffff;
  margin-top: 20rpx;
  padding: 10px;
  font-size: 30rpx;
}
.img-v{
  width: 100%;
  height: 100%;
  background-color: #ffffff;
}
.goodsImages{
  width: 160rpx;
  height: 160rpx;
}
.fa-photo{
  width: 140rpx;
  height: 140rpx;
  margin-left: 22rpx;
  font-size: 100rpx;
  border-radius: 10rpx;
  color: #CECECE;
  text-align: center;
  line-height: 156rpx;
