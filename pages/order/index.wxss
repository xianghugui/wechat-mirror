/* pages/order/index.wxss */

page {
  background-color: #f3f3f3;
}

.content {
  background-color: #f3f3f3;
}

.box-content {
  position: fixed;
  width: 100%;
  height: 100%;
  padding: 0 3.33%;
  background-color: #f3f3f3;
}

.box {
  float: left;
  width: 45%;
  height: 333rpx;
  margin-top: 28rpx;
  background-color: #fff;
  text-align: center;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  box-shadow:1rpx 1rpx 5rpx 0rpx #cecece;
}

image{
  width: 86rpx;
  height: 86rpx;
}

.box>label {
  display: block;
  font-size: 32rpx;
  margin-top: 10rpx;
}
